# Conexión del teclado ASUS Zenbook Duo
ACTION=="add", SUBSYSTEM=="input", KERNEL=="event*", ATTRS{idVendor}=="0b05", ATTRS{idProduct}=="1b2c", ENV{ID_USB_INTERFACE_NUM}=="00", RUN+="/etc/ux8406ma/keyboard-connect-action.sh"

# Desconexión del teclado ASUS Zenbook Duo
ACTION=="remove", SUBSYSTEM=="input", KERNEL=="event*", ATTRS{idVendor}=="0b05", ATTRS{idProduct}=="1b2c", ENV{ID_USB_INTERFACE_NUM}=="00", RUN+="/etc/ux8406ma/keyboard-disconnect-action.sh"